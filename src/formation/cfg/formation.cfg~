#!/usr/bin/env python
PACKAGE = "dynamic_tutorials"

from dynamic_reconfigure.parameter_generator_catkin import *

gen = ParameterGenerator()

gen.add("ENV_K_ALPHA",    double_t,    0, "parameter k_alpha for encirclement control", 2,  0, 30)
gen.add("ENV_K_BETA", double_t, 0, "parameter k_beta for encirclement control",    2, 0,   30)
gen.add("HEI_KP",    double_t,    0, "parameter kp for height control",  2, 0, 30)
gen.add("HEI_KI",    double_t,    0, "parameter ki for height control",  0.02, 0, 10)
gen.add("HEI_KD",    double_t,    0, "parameter kd for height control",  0.3, 0, 5)
gen.add("HEI_BIAS",    double_t,    0, "parameter bias for height control",  0.07, 0, 1)

exit(gen.generate(PACKAGE, "dynamic_tutorials", "Tutorials"))
